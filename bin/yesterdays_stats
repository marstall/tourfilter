mysql --skip-column-names tourfilter_$1 -u$DB_USERNAME -p$DB_PASSWORD -e'select (select count(*) from page_views where date_format(created_at,"%Y/%m/%d")=date_format(adddate(now(),INTERVAL -1 DAY),"%Y/%m/%d") and youser_id<>2 and (source="js" or url="/edit/add" or url="/user/term")) page_views,(select count(distinct session_id) from page_views where date_format(created_at,"%Y/%m/%d")=date_format(adddate(now(),INTERVAL -1 DAY),"%Y/%m/%d") and youser_id<>2 and (source="js" or url="/edit/add" or url="/user/term")) sessions, (select count(*) from users where date_format(last_visited_on,"%Y/%m/%d")=date_format(adddate(now(),INTERVAL -1 DAY),"%Y/%m/%d")) return_visits,(select count(*) from users where date_format(registered_on,"%Y/%m/%d")=date_format(adddate(now(),INTERVAL -1 DAY),"%Y/%m/%d") ) signups;'

