m boston "select terms.text,pages.url,matches.id,matches.status,matches.time_status,matches.date_for_sorting,matches.created_at,matches.day,matches.month from terms,pages,matches where terms.id=matches.term_id and pages.id=matches.page_id and terms.text='$1';"
m boston "select terms.text,left(imported_events.url,50),matches.id,matches.status,matches.time_status,matches.date_for_sorting,matches.created_at,matches.day,matches.month from terms,tourfilter_shared.imported_events,matches where terms.id=matches.term_id and imported_events.id=matches.imported_event_id and terms.text='$1';"

